{
  "title": "Microbiomes",
  "lang": "fr-FR",
  "theme": "#000000",

  "remoteWebSocketServer": "ws://localhost:8080",
  "ghostRemote": [3000, 10000],

  "renderer": {
    "fps": 30,
    "scale": 3
  },

  "scene": {
    "invert": false,
    "padding": 30,
    "patterns": [
      "R-",
      "R-RT",
      "11-22T--3344---55--"
    ],
    "palettes": [
      ["#5181c9", "#49b094", "#5da3d3", "#2b6c4b", "#b79497", "#5b8cde"]
    ]
  },

  "population": {
    "maxLength": 20,
    "initialTypeDistribution": [
      "Builder",
      "Restorer", "Restorer", "Restorer",
      "Shifter", "Shifter", "Shifter", "Shifter", "Shifter", "Shifter"
    ],
    "initialSizeDistribution": [
      25, 25,
      50, 50, 50, 50,
      100, 100,
      200
    ]
  }
}
